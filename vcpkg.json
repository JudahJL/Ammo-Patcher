{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "ammopatcher",
  "version-string": "2.0.0.0",
  "port-version": 0,
  "description": "A simple SKSE plugin built with CommonLibSSE-NG to patch AMMO",
  "license": "Apache-2.0",
  "features": {
    "plugin": {
      "description": "packages required",
      "dependencies": [
        "commonlibsse-ng",
        {
          "name": "rapidjson",
          "version>=": "2023-07-17#2"
        },
        {
          "name": "spdlog",
          "features": [
            "wchar"
          ]
        }
      ]
    }
  },
  "default-features": [
    "plugin"
  ]
}
